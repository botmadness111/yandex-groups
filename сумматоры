class Summator:
    def transform(self, n):
        return n

    def sum(self, N):
        sum = 0
        for i in range(1, n + 1):
            sum = sum + self.transform(i)
        return sum

class SquareSummator(Summator):
    def transform(self, n):
       return n ** 2

class CubeSummator(Summator):
    def transform(self, n):
       return n ** 3
